#, c-format
msgid "indicator variable must have an integer type"
msgstr "指示子変数は整数型でなければなりません"

#: variable.c:506
#, c-format
msgid "unrecognized data type name \"%s\""
msgstr "データ型名\"%s\"は認識できません"

