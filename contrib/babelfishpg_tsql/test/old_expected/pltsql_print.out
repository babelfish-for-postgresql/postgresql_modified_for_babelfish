CREATE FUNCTION multiple_prints_same_line() RETURNS void AS $$
BEGIN
    PRINT 'First PRINT';PRINT 'Second PRINT'
    PRINT 'Third PRINT'; PRINT 'Fourth PRINT'
    PRINT 'Fifth PRINT' PRINT 'Sixth PRINT'
END
$$ LANGUAGE pltsql;
SELECT multiple_prints_same_line();
